CAMERAS
# echo "1" > /proc/sys/net/ipv4/ip_forward
or
# sysctl net.ipv4.ip_forward=1

sudo iptables -t nat -A PREROUTING -p tcp --dport 34599 -j DNAT --to-destination 192.168.15.55:34599
sudo iptables -t nat -A PREROUTING -s 192.168.15.0/24 -p tcp --dport 34599 -j DNAT --to-destination 192.168.15.55:34599
sudo iptables -t nat -A POSTROUTING -j MASQUERADE
sudo iptables-save > /etc/iptables.up.rules
